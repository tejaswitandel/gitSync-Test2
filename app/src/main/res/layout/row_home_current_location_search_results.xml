<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="homeCurrentLocationSearchResultsRowModel" type="com.wqes.app.modules.homecurrentlocationsearchresults.data.model.HomeCurrentLocationSearchResultsRowModel"/>
    </data>
    <FrameLayout android:layout_height="wrap_content"
        android:layout_width="match_parent" style="@style/groupStyle_1">
        <LinearLayout android:layout_gravity="center"
            android:layout_height="wrap_content"
            android:layout_width="match_parent" android:orientation="horizontal">
            <ImageView android:layout_gravity="top"
                android:layout_height="@dimen/_15px"
                android:layout_width="@dimen/_13px" android:src="@drawable/_d74afc1f0e147e2620d74fc330c3af42"/>
            <LinearLayout android:layout_gravity="center_vertical"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_21px"
                android:layout_weight="0.9" android:layout_width="0dp"
                android:orientation="vertical" style="@style/groupStyle_1">
                <TextView android:gravity="top|start"
                    android:id="@+id/txtDawaliMarket"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_1px"
                    android:layout_width="match_parent"
                    android:text="@{homeCurrentLocationSearchResultsRowModel.txtDawaliMarket}" style="@style/textStylePoppinsregular14"/>
                <TextView android:gravity="top|start"
                    android:id="@+id/txtGwarinpaAbuja"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_2px"
                    android:layout_width="match_parent"
                    android:text="@{homeCurrentLocationSearchResultsRowModel.txtGwarinpaAbuja}" style="@style/textStylePoppinslight12_2"/>
            </LinearLayout>
        </LinearLayout>
        <TextView android:gravity="top|start" android:id="@+id/txt10Km"
            android:layout_gravity="center|start"
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:text="@{homeCurrentLocationSearchResultsRowModel.txt10Km}" style="@style/textStylePoppinslight12_2"/>
    </FrameLayout>
</layout>
