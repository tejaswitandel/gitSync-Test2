<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="cartItemAddToCartPopupCustomFood1VM" type="com.wqes.app.modules.cartitemaddtocartpopupcustomfood1.data.viewmodel.CartItemAddToCartPopupCustomFood1VM"/>
    </data>
    <FrameLayout android:layout_height="match_parent"
        android:layout_width="match_parent" style="@style/groupStyleblack_900">
        <LinearLayout android:layout_gravity="bottom|center"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStylewhite_3">
            <LinearLayout android:layout_gravity="center_horizontal"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_19px"
                android:layout_marginStart="@dimen/_19px"
                android:layout_marginTop="@dimen/_43px"
                android:layout_width="wrap_content" android:orientation="horizontal">
                <TextView android:gravity="top|center"
                    android:layout_gravity="center_vertical"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:text="@string/lbl_customize_food" style="@style/textStylePoppinssemibold16"/>
                <ImageView android:layout_gravity="center_vertical"
                    android:layout_height="@dimen/_12px"
                    android:layout_width="@dimen/_12px" android:src="@drawable/_e498e1cca17c6c0d4f05926b7c365c4f"/>
            </LinearLayout>
            <LinearLayout android:layout_gravity="center_horizontal"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_20px"
                android:layout_marginStart="@dimen/_20px"
                android:layout_marginTop="@dimen/_30px"
                android:layout_width="match_parent"
                android:orientation="vertical" style="@style/groupStyle_1">
                <LinearLayout android:layout_gravity="center_horizontal"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <com.google.android.material.card.MaterialCardView
                        android:layout_gravity="center_vertical"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.19"
                        android:layout_width="0dp"
                        app:cardBackgroundColor="@color/black_900"
                        app:cardCornerRadius="@dimen/_5px"
                        app:cardElevation="@dimen/_1px"
                        app:cardUseCompatPadding="true" style="@style/groupStyleblack_900cornerRadius">
                        <ImageView android:layout_gravity="center"
                            android:layout_height="@dimen/_70px"
                            android:layout_width="@dimen/_70px"
                            android:scaleType="fitCenter" imageRes="@{@drawable/_7820b40aec36232f1e9a8422085dabb6960861ac}"/>
                    </com.google.android.material.card.MaterialCardView>
                    <LinearLayout
                        android:layout_gravity="center_vertical"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_14px"
                        android:layout_weight="0.08"
                        android:layout_width="0dp" android:orientation="vertical">
                        <ImageView
                            android:layout_gravity="center_horizontal"
                            android:layout_height="@dimen/_16px" android:layout_width="@dimen/_16px"/>
                        <ImageView
                            android:layout_gravity="center_horizontal"
                            android:layout_height="@dimen/_10px"
                            android:layout_marginEnd="@dimen/_5px"
                            android:layout_marginTop="@dimen/_34px"
                            android:layout_width="@dimen/_10px" android:src="@drawable/_6df562c974c53fb081e73ab0d06492e3"/>
                    </LinearLayout>
                    <LinearLayout android:layout_gravity="bottom"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.22"
                        android:layout_width="0dp" android:orientation="vertical">
                        <TextView android:gravity="top|start"
                            android:layout_gravity="center_horizontal"
                            android:layout_height="wrap_content"
                            android:layout_width="match_parent"
                            android:text="@string/lbl_chinese_pizza" style="@style/textStylePoppinsregular12_1"/>
                        <TextView android:gravity="top|end"
                            android:layout_gravity="start"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_12px"
                            android:layout_marginStart="@dimen/_12px"
                            android:layout_marginTop="@dimen/_3px"
                            android:layout_width="match_parent"
                            android:text="@string/lbl_5_0" style="@style/textStylePoppinsregular12_3"/>
                    </LinearLayout>
                    <LinearLayout
                        android:layout_gravity="center_vertical"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.21"
                        android:layout_width="0dp" android:orientation="vertical">
                        <TextView android:gravity="top|start"
                            android:layout_gravity="center_horizontal"
                            android:layout_height="wrap_content"
                            android:layout_width="match_parent"
                            android:text="@string/lbl_margherita" style="@style/textStylePoppinsregular14"/>
                        <TextView android:gravity="top|start"
                            android:layout_gravity="center_horizontal"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_12px"
                            android:layout_marginTop="@dimen/_27px"
                            android:layout_width="match_parent"
                            android:text="@string/lbl_40_votes" style="@style/textStylePoppinsregular12_4"/>
                    </LinearLayout>
                    <TextView android:layout_gravity="top"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.28" android:layout_width="0dp"/>
                    <ImageView android:layout_gravity="top"
                        android:layout_height="@dimen/_32px"
                        android:layout_marginStart="@dimen/_9px" android:layout_width="@dimen/_32px"/>
                </LinearLayout>
                <TextView android:gravity="top|start"
                    android:layout_gravity="start"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_6px"
                    android:layout_width="wrap_content"
                    android:text="@string/lbl_150" style="@style/textStylePoppinsregular12_7"/>
            </LinearLayout>
            <LinearLayout android:layout_gravity="center_horizontal"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_20px"
                android:layout_marginStart="@dimen/_20px"
                android:layout_marginTop="@dimen/_27px"
                android:layout_width="match_parent"
                android:orientation="horizontal" style="@style/groupStyle_1">
                <TextView android:gravity="top|start"
                    android:layout_gravity="center_vertical"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.73"
                    android:layout_width="0dp"
                    android:text="@string/msg_request_to_rest" style="@style/textStylePoppinssemibold16"/>
                <TextView android:gravity="top|start"
                    android:layout_gravity="center_vertical"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_2px"
                    android:layout_weight="0.27"
                    android:layout_width="0dp"
                    android:text="@string/lbl_optional" style="@style/textStylePoppinsregular12_4"/>
            </LinearLayout>
            <ImageView android:layout_gravity="center_horizontal"
                android:layout_height="@dimen/_53px"
                android:layout_marginEnd="@dimen/_20px"
                android:layout_marginStart="@dimen/_20px"
                android:layout_marginTop="@dimen/_100px"
                android:layout_width="@dimen/_374px" imageRes="@{@drawable/_1a742550ff2536e0253ee896dcc5a3cf}"/>
        </LinearLayout>
        <LinearLayout android:layout_gravity="bottom|center"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_30px"
            android:layout_width="match_parent" android:orientation="vertical">
            <EditText
                android:background="@drawable/oval_bg_black_300_radius_25"
                android:fontFamily="@font/roboto"
                android:gravity="top|start" android:hint="@string/lbl"
                android:id="@+id/etLbl" android:layout_gravity="end"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:paddingBottom="@dimen/_15px"
                android:paddingStart="@dimen/_9px"
                android:paddingTop="@dimen/_16px"
                android:singleLine="true"
                android:textColor="@color/black_900"
                android:textColorHint="@color/black_900"
                android:textFontWeight="400" android:textStyle="normal"/>
            <com.google.android.material.textfield.TextInputLayout
                android:gravity="top|start"
                android:hint="@string/msg_cooking_instruc"
                android:layout_gravity="center_horizontal"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_100px"
                android:layout_width="match_parent"
                android:paddingBottom="@dimen/_30px"
                android:paddingEnd="@dimen/_30px"
                android:paddingTop="@dimen/_1px"
                android:textColorHint="@color/gray_600" style="@style/Widget.MaterialComponents.TextInputLayout.FilledBox">
                <com.google.android.material.textfield.TextInputEditText
                    android:fontFamily="@font/poppins"
                    android:id="@+id/etCookingInstruc"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:singleLine="true"
                    android:text="@string/msg_cooking_instruc"
                    android:textColor="@color/gray_600"
                    android:textFontWeight="400"
                    android:textSize="@dimen/_14px" android:textStyle="normal"/>
            </com.google.android.material.textfield.TextInputLayout>
            <androidx.appcompat.widget.AppCompatButton
                android:gravity="center" android:id="@+id/btnAdd150"
                android:layout_gravity="center_horizontal"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_30px"
                android:layout_width="match_parent"
                android:paddingBottom="@dimen/_12px"
                android:paddingEnd="@dimen/_30px"
                android:paddingStart="@dimen/_30px"
                android:paddingTop="@dimen/_15px"
                android:text="@string/lbl_add_150" style="@style/textStylePoppinsmedium14_1"/>
        </LinearLayout>
    </FrameLayout>
</layout>
