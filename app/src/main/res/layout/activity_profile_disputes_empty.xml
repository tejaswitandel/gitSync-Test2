<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="profileDisputesEmptyVM" type="com.wqes.app.modules.profiledisputesempty.data.viewmodel.ProfileDisputesEmptyVM"/>
    </data>
    <LinearLayout android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" android:paddingEnd="@dimen/_20px"
        android:paddingStart="@dimen/_20px" style="@style/groupStylewhite_A700">
        <androidx.appcompat.widget.Toolbar android:id="@+id/toolbar"
            android:layout_height="wrap_content" android:layout_width="match_parent">
            <LinearLayout android:layout_gravity="center_horizontal"
                android:layout_height="@dimen/_22px"
                android:layout_width="match_parent"
                android:orientation="horizontal" style="@style/groupStylered_300">
                <ImageView android:layout_gravity="center_vertical"
                    android:layout_height="@dimen/_17px"
                    android:layout_width="@dimen/_10px" android:src="@drawable/_2d7e37f470e288bf99df74248282992f"/>
                <TextView android:gravity="top|start"
                    android:layout_gravity="center_vertical"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_23px"
                    android:layout_weight="0.44"
                    android:layout_width="0dp"
                    android:text="@string/lbl_raise_a_dispute" style="@style/textStylePoppinsmedium18_1"/>
                <TextView android:gravity="top|end"
                    android:layout_gravity="center_vertical"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.5"
                    android:layout_width="0dp"
                    android:text="@string/lbl_my_disputes" style="@style/textStylePoppinsmedium14_7"/>
            </LinearLayout>
        </androidx.appcompat.widget.Toolbar>
        <ScrollView android:fillViewport="true"
            android:layout_height="0dp"
            android:layout_marginTop="@dimen/_100px"
            android:layout_weight="1" android:layout_width="match_parent">
            <LinearLayout android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="vertical">
                <ImageView android:layout_gravity="center_horizontal"
                    android:layout_height="@dimen/_266px"
                    android:layout_marginEnd="@dimen/_48px"
                    android:layout_marginStart="@dimen/_48px"
                    android:layout_width="@dimen/_266px" imageRes="@{@drawable/_df554e74be1e590c832ad030c037898b}"/>
                <TextView android:gravity="top|center"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_9px"
                    android:layout_width="match_parent"
                    android:text="@string/msg_you_didn_t_orde" style="@style/textStylePoppinsmedium18"/>
                <ImageView android:layout_gravity="center_horizontal"
                    android:layout_height="@dimen/_24px"
                    android:layout_marginTop="@dimen/_100px"
                    android:layout_width="@dimen/_24px" android:src="@drawable/_ec5cce601ca7e7a6fb08ade4d8b4489a"/>
            </LinearLayout>
        </ScrollView>
    </LinearLayout>
</layout>
