                        android:layout_height="wrap_content"
                        android:layout_weight="0.67"
                        android:layout_width="0dp"
                        android:orientation="horizontal" style="@style/groupStyle_1">
                        <ImageView
                            android:layout_gravity="center_vertical"
                            android:layout_height="@dimen/_17px"
                            android:layout_width="@dimen/_26px" android:src="@drawable/_6cbcd3a2ea2c54e78ca6e66ebdef63ae"/>
                        <TextView android:gravity="top|end"
                            android:layout_gravity="center_vertical"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_6px"
                            android:layout_weight="0.22"
                            android:layout_width="0dp"
                            android:text="@string/lbl_234" style="@style/textStylePoppinsmedium14_3"/>
                        <ImageView
                            android:layout_gravity="center_vertical"
                            android:layout_height="@dimen/_4px"
                            android:layout_marginStart="@dimen/_3px"
                            android:layout_width="@dimen/_7px" android:src="@drawable/_33150964fc88d1b9425e1d0dfb71d053"/>
                        <View android:background="@color/black_300"
                            android:layout_gravity="center_vertical"
                            android:layout_height="@dimen/_23px"
                            android:layout_marginStart="@dimen/_10px"
                            android:layout_weight="0.06" android:layout_width="0dp"/>
                        <TextView android:gravity="top|end"
                            android:layout_gravity="center_vertical"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_6px"
                            android:layout_weight="0.51"
                            android:layout_width="0dp"
                            android:text="@string/lbl_9135715252" style="@style/textStylePoppinsmedium16_1"/>
                    </LinearLayout>
                </LinearLayout>
                <LinearLayout android:layout_gravity="center_horizontal"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_20px"
                    android:layout_marginStart="@dimen/_20px"
                    android:layout_marginTop="@dimen/_3px"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <TextView android:gravity="top|start"
                        android:layout_gravity="center_vertical"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.2"
                        android:layout_width="0dp"
                        android:text="@string/lbl_category" style="@style/textStylePoppinsregular16_3"/>
                    <TextView android:gravity="top|end"
                        android:layout_gravity="center_vertical"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.51"
                        android:layout_width="0dp"
                        android:text="@string/lbl_vegetarian" style="@style/textStylePoppinsmedium16_1"/>
                </LinearLayout>
                <LinearLayout android:layout_gravity="center_horizontal"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_20px"
                    android:layout_marginStart="@dimen/_20px"
                    android:layout_marginTop="@dimen/_21px"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <TextView android:gravity="top|start"
                        android:layout_gravity="center_vertical"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.16"
                        android:layout_width="0dp"
                        android:text="@string/lbl_cuisine" style="@style/textStylePoppinsregular16_3"/>
                    <TextView android:gravity="top|end"
                        android:layout_gravity="center_vertical"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.8"
                        android:layout_width="0dp"
                        android:text="@string/msg_nigerian_seafo" style="@style/textStylePoppinsmedium16_1"/>
                </LinearLayout>
                <LinearLayout android:layout_gravity="center_horizontal"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_20px"
                    android:layout_marginStart="@dimen/_20px"
                    android:layout_marginTop="@dimen/_21px"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <TextView android:gravity="top|start"
                        android:layout_gravity="center_vertical"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.22"
                        android:layout_width="0dp"
                        android:text="@string/lbl_open_now" style="@style/textStylePoppinsregular16_3"/>
                    <TextView android:gravity="top|end"
                        android:layout_gravity="center_vertical"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.73"
                        android:layout_width="0dp"
                        android:text="@string/msg_10_30am_10_301" style="@style/textStylePoppinsmedium16_1"/>
                    <ImageView android:layout_gravity="center_vertical"
                        android:layout_height="@dimen/_23px"
                        android:layout_width="@dimen/_17px" android:src="@drawable/_3e1fd535d2415067e13df71b9273418a"/>
