<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="profileChangePasswordPopupVM" type="com.wqes.app.modules.profilechangepasswordpopup.data.viewmodel.ProfileChangePasswordPopupVM"/>
    </data>
    <FrameLayout android:layout_height="match_parent"
        android:layout_width="match_parent" style="@style/groupStyleblack_900">
        <LinearLayout android:layout_gravity="center"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStylewhite_4">
            <TextView android:gravity="top|center"
                android:layout_gravity="center_horizontal"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_29px"
                android:layout_width="wrap_content"
                android:text="@string/msg_current_passwor" style="@style/textStylePoppinssemibold18"/>
            <TextView android:gravity="top|center"
                android:layout_gravity="center_horizontal"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_17px"
                android:layout_width="wrap_content"
                android:text="@string/msg_existing_passwo" style="@style/textStylePoppinsregular14_1"/>
            <ImageView android:layout_gravity="center_horizontal"
                android:layout_height="@dimen/_53px"
                android:layout_marginEnd="@dimen/_42px"
                android:layout_marginStart="@dimen/_42px"
                android:layout_marginTop="@dimen/_19px"
                android:layout_width="@dimen/_290px" imageRes="@{@drawable/_5f747cf0e92b1fabc1b23389e63f666f}"/>
        </LinearLayout>
        <androidx.appcompat.widget.AppCompatButton
            android:gravity="center" android:id="@+id/btnTryAgain"
            android:layout_gravity="center"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:paddingBottom="@dimen/_12px"
            android:paddingEnd="@dimen/_30px"
            android:paddingStart="@dimen/_30px"
            android:paddingTop="@dimen/_15px"
            android:text="@string/lbl_try_again" style="@style/textStylePoppinsmedium14_1"/>
    </FrameLayout>
</layout>
