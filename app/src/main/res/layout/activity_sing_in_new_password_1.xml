<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="singInNewPassword1VM" type="com.wqes.app.modules.singinnewpassword1.data.viewmodel.SingInNewPassword1VM"/>
    </data>
    <LinearLayout android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylewhite_A700">
        <LinearLayout android:layout_gravity="center_horizontal"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_100px"
            android:layout_width="wrap_content"
            android:orientation="vertical" style="@style/groupStyle_1">
            <ImageView android:layout_gravity="center_horizontal"
                android:layout_height="@dimen/_115px"
                android:layout_width="@dimen/_119px" imageRes="@{@drawable/_e1120f99e923caedaa63dd17734a1cfe}"/>
            <TextView android:gravity="top|center"
                android:layout_gravity="center_horizontal"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_1px"
                android:layout_marginStart="@dimen/_1px"
                android:layout_marginTop="@dimen/_21px"
                android:layout_width="wrap_content"
                android:text="@string/msg_password_reset" style="@style/textStylePoppinsmedium18"/>
        </LinearLayout>
        <androidx.appcompat.widget.AppCompatButton
            android:gravity="center" android:id="@+id/btnBackToSignIn"
            android:layout_gravity="center_horizontal"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_20px"
            android:layout_marginStart="@dimen/_20px"
            android:layout_marginTop="@dimen/_100px"
            android:layout_width="match_parent"
            android:paddingBottom="@dimen/_12px"
            android:paddingEnd="@dimen/_30px"
            android:paddingStart="@dimen/_30px"
            android:paddingTop="@dimen/_15px"
            android:text="@string/lbl_back_to_sign_in" style="@style/textStylePoppinsmedium14_1"/>
    </LinearLayout>
</layout>
