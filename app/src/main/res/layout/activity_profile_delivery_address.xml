<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="profileDeliveryAddressVM" type="com.wqes.app.modules.profiledeliveryaddress.data.viewmodel.ProfileDeliveryAddressVM"/>
    </data>
    <LinearLayout android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" android:paddingEnd="@dimen/_30px" style="@style/groupStylewhite_A700">
        <androidx.appcompat.widget.Toolbar android:id="@+id/toolbar"
            android:layout_height="wrap_content" android:layout_width="match_parent">
            <LinearLayout android:layout_gravity="center_horizontal"
                android:layout_height="@dimen/_22px"
                android:layout_width="match_parent"
                android:orientation="horizontal" style="@style/groupStylered_300">
                <ImageView android:layout_gravity="center_vertical"
                    android:layout_height="@dimen/_17px"
                    android:layout_width="@dimen/_10px" android:src="@drawable/_2d7e37f470e288bf99df74248282992f"/>
                <TextView android:gravity="top|start"
                    android:layout_gravity="center_vertical"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_23px"
                    android:layout_weight="0.94"
                    android:layout_width="0dp"
                    android:text="@string/msg_delivery_addres" style="@style/textStylePoppinsmedium18_1"/>
            </LinearLayout>
        </androidx.appcompat.widget.Toolbar>
        <ScrollView android:fillViewport="true"
            android:layout_height="0dp"
            android:layout_marginTop="@dimen/_47px"
            android:layout_weight="1" android:layout_width="match_parent">
            <LinearLayout android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="vertical">
                <LinearLayout android:layout_gravity="center_horizontal"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:orientation="vertical" style="@style/groupStylewhite_A700cornerRadius">
                    <LinearLayout
                        android:layout_gravity="center_horizontal"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_20px"
                        android:layout_marginStart="@dimen/_20px"
                        android:layout_marginTop="@dimen/_20px"
                        android:layout_width="match_parent" android:orientation="horizontal">
                        <TextView android:gravity="top|start"
                            android:layout_gravity="center_vertical"
                            android:layout_height="wrap_content"
                            android:layout_weight="0.13"
                            android:layout_width="0dp"
                            android:text="@string/lbl_home" style="@style/textStylePoppinsmedium14_3"/>
                        <TextView android:gravity="top|end"
                            android:layout_gravity="center_vertical"
                            android:layout_height="wrap_content"
                            android:layout_weight="0.45"
                            android:layout_width="0dp"
                            android:text="@string/lbl_default" style="@style/textStylePoppinsregular14_2"/>
                    </LinearLayout>
                    <LinearLayout
                        android:layout_gravity="center_horizontal"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_19px"
                        android:layout_marginStart="@dimen/_19px"
                        android:layout_marginTop="@dimen/_8px"
                        android:layout_width="match_parent" android:orientation="horizontal">
                        <ImageView
                            android:layout_gravity="center_vertical"
                            android:layout_height="@dimen/_15px"
                            android:layout_width="@dimen/_13px" android:src="@drawable/_cec47a15119a01d1b0162e197aef1c97"/>
                        <TextView android:gravity="top|start"
                            android:layout_gravity="center_vertical"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_11px"
                            android:layout_weight="0.96"
                            android:layout_width="0dp"
                            android:text="@string/msg_sentosa_park_an" style="@style/textStylePoppinsregular12_1"/>
                    </LinearLayout>
                </LinearLayout>
                <HorizontalScrollView android:fillViewport="true"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_20px"
                    android:layout_width="match_parent" android:scrollbars="none">
                    <FrameLayout
                        android:layout_gravity="center_horizontal"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content" style="@style/groupStyle_1">
                        <FrameLayout android:layout_gravity="center"
                            android:layout_height="wrap_content"
                            android:layout_width="match_parent" style="@style/groupStylered_50cornerRadius">
                            <FrameLayout
                                android:background="@drawable/rectangle_bg_white_a700_radius_15"
                                android:layout_gravity="center"
                                android:layout_height="wrap_content" android:layout_width="match_parent"/>
                            <LinearLayout
                                android:layout_gravity="center"
                                android:layout_height="wrap_content"
                                android:layout_width="match_parent" android:orientation="vertical">
                                <TextView android:gravity="top|start"
                                    android:layout_gravity="start"
                                    android:layout_height="wrap_content"
                                    android:layout_marginEnd="@dimen/_2px"
                                    android:layout_marginStart="@dimen/_2px"
                                    android:layout_width="wrap_content"
                                    android:text="@string/lbl_beta_house" style="@style/textStylePoppinsmedium14_3"/>
                                <LinearLayout
                                    android:layout_gravity="center_horizontal"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="@dimen/_8px"
                                    android:layout_width="match_parent" android:orientation="horizontal">
                                    <ImageView
                                    android:layout_gravity="center_vertical"
                                    android:layout_height="@dimen/_14px"
                                    android:layout_width="@dimen/_11px" android:src="@drawable/_6871ae147557d5306b3234234405beb6"/>
                                    <TextView
                                    android:gravity="top|start"
                                    android:layout_gravity="center_vertical"
                                    android:layout_height="wrap_content"
                                    android:layout_marginStart="@dimen/_14px"
                                    android:layout_weight="0.96"
                                    android:layout_width="0dp"
                                    android:text="@string/msg_zone_a_legisla" style="@style/textStylePoppinsregular12_1"/>
                                </LinearLayout>
                            </LinearLayout>
                        </FrameLayout>
                        <FrameLayout android:layout_gravity="center|end"
                            android:layout_height="wrap_content"
                            android:layout_width="wrap_content" style="@style/groupStyle_1">
                            <LinearLayout
                                android:layout_gravity="center"
                                android:layout_height="match_parent"
                                android:layout_marginBottom="@dimen/_22px"
                                android:layout_marginEnd="@dimen/_20px"
                                android:layout_marginStart="@dimen/_20px"
                                android:layout_marginTop="@dimen/_26px"
                                android:layout_width="match_parent"
                                android:orientation="vertical" style="@style/groupStylered_300cornerRadius">
                                <ImageView
                                    android:layout_gravity="center_horizontal"
                                    android:layout_height="@dimen/_16px"
                                    android:layout_marginEnd="@dimen/_13px"
                                    android:layout_marginStart="@dimen/_13px"
                                    android:layout_width="@dimen/_13px" android:src="@drawable/_6e3dc12d526bf65626ed08164cad1103"/>
                                <TextView android:gravity="top|center"
                                    android:layout_gravity="center_horizontal"
                                    android:layout_height="wrap_content"
                                    android:layout_width="match_parent"
                                    android:text="@string/lbl_delete" style="@style/textStylePoppinsregular12_2"/>
                            </LinearLayout>
                        </FrameLayout>
                    </FrameLayout>
                </HorizontalScrollView>
                <HorizontalScrollView android:fillViewport="true"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_20px"
                    android:layout_width="match_parent" android:scrollbars="none">
                    <FrameLayout
                        android:layout_gravity="center_horizontal"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content" style="@style/groupStyle_1">
                        <FrameLayout android:layout_gravity="center"
                            android:layout_height="wrap_content"
                            android:layout_width="match_parent" style="@style/groupStylegreen_50cornerRadius">
                            <FrameLayout
                                android:background="@drawable/rectangle_bg_white_a700_radius_15"
                                android:layout_gravity="center"
                                android:layout_height="wrap_content" android:layout_width="match_parent"/>
                            <LinearLayout
                                android:layout_gravity="center"
                                android:layout_height="wrap_content"
                                android:layout_width="wrap_content" android:orientation="vertical">
                                <TextView android:gravity="top|start"
                                    android:layout_gravity="start"
                                    android:layout_height="wrap_content"
                                    android:layout_marginEnd="@dimen/_1px"
                                    android:layout_marginStart="@dimen/_1px"
                                    android:layout_width="wrap_content"
                                    android:text="@string/lbl_office" style="@style/textStylePoppinsmedium14_3"/>
                                <LinearLayout
                                    android:layout_gravity="center_horizontal"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="@dimen/_8px"
                                    android:layout_width="wrap_content" android:orientation="horizontal">
                                    <ImageView
                                    android:layout_gravity="center_vertical"
                                    android:layout_height="@dimen/_15px"
                                    android:layout_width="@dimen/_13px" android:src="@drawable/_cec47a15119a01d1b0162e197aef1c97"/>
                                    <TextView
                                    android:gravity="top|start"
                                    android:layout_gravity="center_vertical"
                                    android:layout_height="wrap_content"
                                    android:layout_marginStart="@dimen/_11px"
                                    android:layout_width="wrap_content"
                                    android:text="@string/msg_4_pp_market_g" style="@style/textStylePoppinsregular12_1"/>
                                </LinearLayout>
                            </LinearLayout>
                        </FrameLayout>
                        <FrameLayout
                            android:layout_gravity="center|start"
                            android:layout_height="wrap_content"
                            android:layout_width="wrap_content" style="@style/groupStyle_1">
                            <LinearLayout
                                android:layout_gravity="center"
                                android:layout_height="match_parent"
                                android:layout_marginBottom="@dimen/_22px"
                                android:layout_marginEnd="@dimen/_29px"
                                android:layout_marginStart="@dimen/_29px"
                                android:layout_marginTop="@dimen/_26px"
                                android:layout_width="match_parent"
                                android:orientation="vertical" style="@style/groupStylegreen_700cornerRadius">
                                <ImageView
                                    android:layout_gravity="center_horizontal"
                                    android:layout_height="@dimen/_16px"
                                    android:layout_marginEnd="@dimen/_2px"
                                    android:layout_marginStart="@dimen/_2px"
                                    android:layout_width="@dimen/_16px" android:src="@drawable/_e0dc9de80bc7b51dd8b0e67286e7bf06"/>
                                <TextView android:gravity="top|center"
                                    android:layout_gravity="center_horizontal"
                                    android:layout_height="wrap_content"
                                    android:layout_width="match_parent"
                                    android:text="@string/lbl_edit" style="@style/textStylePoppinsregular12_8"/>
                            </LinearLayout>
                        </FrameLayout>
                    </FrameLayout>
                </HorizontalScrollView>
                <LinearLayout android:layout_gravity="center_horizontal"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_20px"
                    android:layout_width="match_parent"
                    android:orientation="vertical" style="@style/groupStylewhite_A700cornerRadius">
                    <TextView android:gravity="top|start"
                        android:layout_gravity="start"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_20px"
                        android:layout_marginStart="@dimen/_20px"
                        android:layout_marginTop="@dimen/_20px"
                        android:layout_width="wrap_content"
                        android:text="@string/lbl_farm" style="@style/textStylePoppinsmedium14_3"/>
                    <LinearLayout
                        android:layout_gravity="center_horizontal"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_20px"
                        android:layout_marginStart="@dimen/_20px"
                        android:layout_marginTop="@dimen/_8px"
                        android:layout_width="match_parent" android:orientation="horizontal">
                        <ImageView
                            android:layout_gravity="center_vertical"
                            android:layout_height="@dimen/_15px"
                            android:layout_width="@dimen/_13px" android:src="@drawable/_cec47a15119a01d1b0162e197aef1c97"/>
                        <TextView android:gravity="top|start"
                            android:layout_gravity="center_vertical"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_10px"
                            android:layout_weight="0.94"
                            android:layout_width="0dp"
                            android:text="@string/msg_1_food_wingso" style="@style/textStylePoppinsregular12_1"/>
                    </LinearLayout>
                </LinearLayout>
                <androidx.appcompat.widget.AppCompatButton
                    android:gravity="bottom|center"
                    android:id="@+id/btnAddNewAddress"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_100px"
                    android:layout_width="match_parent"
                    android:paddingEnd="@dimen/_30px"
                    android:paddingStart="@dimen/_30px"
                    android:paddingTop="@dimen/_15px"
                    android:text="@string/lbl_add_new_address" style="@style/textStylePoppinsmedium14_1"/>
            </LinearLayout>
        </ScrollView>
    </LinearLayout>
</layout>
