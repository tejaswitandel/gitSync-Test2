<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="singInNewPasswordDataErrorVM" type="com.wqes.app.modules.singinnewpassworddataerror.data.viewmodel.SingInNewPasswordDataErrorVM"/>
    </data>
    <LinearLayout android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylewhite_A700">
        <ImageView android:layout_gravity="start"
            android:layout_height="@dimen/_17px"
            android:layout_marginEnd="@dimen/_20px"
            android:layout_marginStart="@dimen/_20px"
            android:layout_marginTop="@dimen/_60px"
            android:layout_width="@dimen/_10px" android:src="@drawable/_f3ee32990476ecf23ff68e15dcc5dc46"/>
        <TextView android:gravity="top|start"
            android:layout_gravity="start"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_20px"
            android:layout_marginStart="@dimen/_20px"
            android:layout_marginTop="@dimen/_76px"
            android:layout_width="wrap_content"
            android:text="@string/lbl_new_password" style="@style/textStylePoppinssemibold20"/>
        <TextView android:gravity="top|start"
            android:layout_gravity="center_horizontal"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_20px"
            android:layout_marginStart="@dimen/_20px"
            android:layout_marginTop="@dimen/_11px"
            android:layout_width="match_parent"
            android:text="@string/msg_set_your_new_pa" style="@style/textStylePoppinsregular14_11"/>
        <FrameLayout android:layout_gravity="center_horizontal"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_20px"
            android:layout_marginStart="@dimen/_20px"
            android:layout_marginTop="@dimen/_39px"
            android:layout_width="match_parent" style="@style/groupStyle_1">
            <FrameLayout android:layout_gravity="center"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" style="@style/groupStyle_1">
                <ImageView android:layout_gravity="center"
                    android:layout_height="@dimen/_63px"
                    android:layout_width="@dimen/_374px" imageRes="@{@drawable/_feb8af1f1b7452e31ca48ed4ac483f5b}"/>
                <LinearLayout android:layout_gravity="center|start"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content" android:orientation="vertical">
                    <LinearLayout
                        android:layout_gravity="center_horizontal"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content" android:orientation="horizontal">
                        <TextView android:gravity="top|start"
                            android:layout_gravity="center_vertical"
                            android:layout_height="wrap_content"
                            android:layout_width="wrap_content"
                            android:text="@string/lbl_new_password" style="@style/textStylePoppinsregular14_1"/>
                        <ImageView
                            android:layout_gravity="center_vertical"
                            android:layout_height="@dimen/_20px"
                            android:layout_marginStart="@dimen/_7px"
                            android:layout_width="@dimen/_20px" android:src="@drawable/_7a3bd977439e82656d899150d93833bc"/>
                    </LinearLayout>
                    <ImageView android:layout_gravity="start"
                        android:layout_height="@dimen/_9px"
                        android:layout_marginEnd="@dimen/_47px"
                        android:layout_marginTop="@dimen/_17px"
                        android:layout_width="@dimen/_88px" android:src="@drawable/_78c830525a548789980dbd1651ac3a4f"/>
                </LinearLayout>
            </FrameLayout>
            <LinearLayout android:layout_gravity="bottom|center"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:orientation="vertical" style="@style/groupStyle_1">
                <LinearLayout android:layout_gravity="center_horizontal"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <FrameLayout android:background="@color/black_900"
                        android:layout_gravity="center_vertical"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.24" android:layout_width="0dp"/>
                    <ImageView android:layout_gravity="center_vertical"
                        android:layout_height="@dimen/_11px"
                        android:layout_width="@dimen/_17px" android:src="@drawable/_13b4b7ee58f8f170779fc8695206e016"/>
                </LinearLayout>
                <TextView android:gravity="top|start"
                    android:layout_gravity="start"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:text="@string/lbl" style="@style/textStyleRobotoregular9"/>
                <View android:background="@color/bluegray_200"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="@dimen/_1px"
                    android:layout_marginTop="@dimen/_6px" android:layout_width="match_parent"/>
            </LinearLayout>
        </FrameLayout>
        <TextView android:gravity="top|start"
            android:layout_gravity="start"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_20px"
            android:layout_marginStart="@dimen/_20px"
            android:layout_marginTop="@dimen/_11px"
            android:layout_width="match_parent"
            android:text="@string/msg_please_enter_va" style="@style/textStylePoppinsmedium14_10"/>
        <com.google.android.material.textfield.TextInputLayout
            android:gravity="top|start"
            android:hint="@string/msg_confirm_passwor"
            android:layout_gravity="center_horizontal"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_20px"
            android:layout_marginStart="@dimen/_20px"
            android:layout_marginTop="@dimen/_19px"
            android:layout_width="match_parent"
            android:paddingBottom="@dimen/_30px"
            android:paddingEnd="@dimen/_30px"
            android:paddingTop="@dimen/_1px"
            android:textColorHint="@color/gray_600" style="@style/Widget.MaterialComponents.TextInputLayout.FilledBox">
            <com.google.android.material.textfield.TextInputEditText
                android:fontFamily="@font/poppins"
                android:id="@+id/etConfirmPasswor"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:singleLine="true"
                android:text="@string/msg_confirm_passwor"
                android:textColor="@color/gray_600"
                android:textFontWeight="400"
                android:textSize="@dimen/_14px" android:textStyle="normal"/>
        </com.google.android.material.textfield.TextInputLayout>
        <androidx.appcompat.widget.AppCompatButton
            android:gravity="center" android:id="@+id/btnSave"
            android:layout_gravity="center_horizontal"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_20px"
            android:layout_marginStart="@dimen/_20px"
            android:layout_marginTop="@dimen/_92px"
            android:layout_width="match_parent"
            android:paddingBottom="@dimen/_12px"
            android:paddingEnd="@dimen/_30px"
            android:paddingStart="@dimen/_30px"
            android:paddingTop="@dimen/_15px"
            android:text="@string/lbl_save" style="@style/textStylePoppinsmedium14_1"/>
    </LinearLayout>
</layout>
