                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_6px"
                android:layout_weight="0.34" android:layout_width="0dp"
                android:text="@{homeRowModel.txt460FoodR}" style="@style/textStylePoppinsregular12_4"/>
        </LinearLayout>
        <TextView android:gravity="top|start"
