                <View android:background="@color/black_300"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="@dimen/_1px"
                    android:layout_marginTop="@dimen/_28px" android:layout_width="match_parent"/>
                <com.google.android.material.textfield.TextInputLayout
                    android:gravity="top|start"
                    android:hint="@string/lbl_note"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_20px"
                    android:layout_marginStart="@dimen/_20px"
                    android:layout_marginTop="@dimen/_30px"
                    android:layout_width="match_parent"
                    android:paddingBottom="@dimen/_5px"
                    android:paddingTop="@dimen/_30px"
                    android:textColorHint="@color/red_300"
                    app:boxBackgroundColor="@null" style="@style/Widget.MaterialComponents.TextInputLayout.FilledBox">

                    <com.google.android.material.textfield.TextInputEditText
                        android:fontFamily="@font/poppins"
                        android:id="@+id/etNote"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:paddingStart="0dp"
                        android:singleLine="true"
                        android:text="@string/msg_launch_party_wa"
                        android:textColor="@color/black_800"
                        android:textFontWeight="500"
                        android:textSize="@dimen/_16px" android:textStyle="bold"/>
                </com.google.android.material.textfield.TextInputLayout>
            </LinearLayout>
        </ScrollView>
    </LinearLayout>
</layout>
