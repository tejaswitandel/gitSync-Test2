    <data>
        <variable name="profilePaymentMethod1VM" type="com.wqes.app.modules.profilepaymentmethod1.data.viewmodel.ProfilePaymentMethod1VM"/>
    </data>
    <LinearLayout android:layout_height="match_parent"
