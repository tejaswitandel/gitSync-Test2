<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="cartEmptyScreenVM" type="com.wqes.app.modules.cartemptyscreen.data.viewmodel.CartEmptyScreenVM"/>
    </data>
    <LinearLayout android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylewhite_A700">
        <ScrollView android:fillViewport="true"
            android:layout_height="0dp" android:layout_weight="1" android:layout_width="match_parent">
            <LinearLayout android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="vertical">
                <TextView android:gravity="bottom|start"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:paddingBottom="@dimen/_17px"
                    android:paddingStart="@dimen/_18px"
                    android:paddingTop="@dimen/_30px"
                    android:text="@string/lbl_cart" style="@style/textStylePoppinsmedium18_2"/>
                <ImageView android:layout_gravity="center_horizontal"
                    android:layout_height="@dimen/_266px"
                    android:layout_marginTop="@dimen/_100px"
                    android:layout_width="@dimen/_266px" imageRes="@{@drawable/_c3385ef795e2a3f49572401c13714edb}"/>
                <TextView android:gravity="top|center"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_13px"
                    android:layout_width="wrap_content"
                    android:text="@string/msg_your_cart_is_em" style="@style/textStylePoppinsmedium18"/>
                <TextView android:gravity="top|center"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_11px"
                    android:layout_width="wrap_content"
                    android:text="@string/msg_add_items_in_ca" style="@style/textStylePoppinsregular14_1"/>
            </LinearLayout>
        </ScrollView>
        <FrameLayout android:layout_gravity="center_horizontal"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_100px"
            android:layout_width="match_parent" style="@style/groupStyle_1">
            <ImageView android:layout_gravity="center"
                android:layout_height="@dimen/_80px" android:layout_width="@dimen/_413px"/>
            <FrameLayout android:background="@color/white_A700"
                android:layout_gravity="center|end"
                android:layout_height="wrap_content" android:layout_width="wrap_content"/>
            <LinearLayout android:layout_gravity="center|start"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_40px"
                android:layout_width="wrap_content"
                android:orientation="vertical" style="@style/groupStyle_1">
                <ImageView android:layout_gravity="center_horizontal"
                    android:layout_height="@dimen/_22px"
                    android:layout_marginEnd="@dimen/_11px"
                    android:layout_marginStart="@dimen/_11px"
                    android:layout_width="@dimen/_21px" android:src="@drawable/_1f1fb5efbbd3ed192a14f5fabf592036"/>
                <TextView android:gravity="top|center"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_1px"
                    android:layout_marginTop="@dimen/_9px"
                    android:layout_width="wrap_content"
                    android:text="@string/lbl_home" style="@style/textStylePoppinsregular14_1"/>
            </LinearLayout>
            <LinearLayout android:layout_gravity="center"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:orientation="vertical" style="@style/groupStyle_1">
                <ImageView android:layout_gravity="center_horizontal"
                    android:layout_height="@dimen/_24px"
                    android:layout_marginEnd="@dimen/_27px"
                    android:layout_marginStart="@dimen/_27px"
                    android:layout_width="@dimen/_16px" android:src="@drawable/_c5fbfe8b7cd040d93ed8be35b9576f98"/>
                <TextView android:gravity="top|center"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_6px"
                    android:layout_width="wrap_content"
                    android:text="@string/lbl_bookmark" style="@style/textStylePoppinsregular14_1"/>
            </LinearLayout>
            <LinearLayout android:layout_gravity="center|end"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_39px"
                android:layout_width="wrap_content"
                android:orientation="vertical" style="@style/groupStyle_1">
                <ImageView android:layout_gravity="center_horizontal"
                    android:layout_height="@dimen/_21px"
                    android:layout_marginEnd="@dimen/_11px"
                    android:layout_marginStart="@dimen/_11px"
                    android:layout_width="@dimen/_22px" android:src="@drawable/_6d7676b362dc3b4be477d768e46a0635"/>
                <TextView android:gravity="top|center"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_9px"
                    android:layout_width="wrap_content"
                    android:text="@string/lbl_profile" style="@style/textStylePoppinsregular14_1"/>
            </LinearLayout>
            <LinearLayout android:layout_gravity="center"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:orientation="vertical" style="@style/groupStyle_1">
                <ImageView android:layout_gravity="center_horizontal"
                    android:layout_height="@dimen/_22px"
                    android:layout_marginEnd="@dimen/_7px"
                    android:layout_marginStart="@dimen/_7px"
                    android:layout_width="@dimen/_17px" android:src="@drawable/_56dda2f47daeae192bc8f7486170221f"/>
                <TextView android:gravity="top|center"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_8px"
                    android:layout_width="wrap_content"
                    android:text="@string/lbl_cart" style="@style/textStylePoppinsregular14_2"/>
            </LinearLayout>
        </FrameLayout>
    </LinearLayout>
</layout>
