<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="singInMobileNumberUnregisteredDataPopupVM" type="com.wqes.app.modules.singinmobilenumberunregistereddatapopup.data.viewmodel.SingInMobileNumberUnregisteredDataPopupVM"/>
    </data>
    <FrameLayout android:layout_height="match_parent"
        android:layout_width="match_parent" style="@style/groupStyleblack_900">
        <LinearLayout android:layout_gravity="center"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStylewhite_4">
            <TextView android:gravity="top|center"
                android:layout_gravity="center_horizontal"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_29px"
                android:layout_width="wrap_content"
                android:text="@string/lbl_sign_in_error" style="@style/textStylePoppinssemibold18"/>
            <TextView android:gravity="top|center"
                android:layout_gravity="center_horizontal"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_31px"
                android:layout_marginStart="@dimen/_31px"
                android:layout_marginTop="@dimen/_17px"
                android:layout_width="match_parent"
                android:text="@string/msg_unregistered_mo" style="@style/textStylePoppinsregular14_1"/>
            <ImageView android:layout_gravity="center_horizontal"
                android:layout_height="@dimen/_53px"
                android:layout_marginEnd="@dimen/_42px"
                android:layout_marginStart="@dimen/_42px"
                android:layout_marginTop="@dimen/_19px"
                android:layout_width="@dimen/_290px" imageRes="@{@drawable/_9e4fc39a7d4e25a94fbb795513ff5c71}"/>
        </LinearLayout>
        <androidx.appcompat.widget.AppCompatButton
            android:gravity="center" android:id="@+id/btnSignUp"
            android:layout_gravity="center"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:paddingBottom="@dimen/_12px"
            android:paddingEnd="@dimen/_30px"
            android:paddingStart="@dimen/_30px"
            android:paddingTop="@dimen/_15px"
            android:text="@string/lbl_sign_up" style="@style/textStylePoppinsmedium14_1"/>
    </FrameLayout>
</layout>
